{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ],
  "rules": {
    "only-arrow-functions": false
  }
}